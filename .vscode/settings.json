{
  "i18n-ally.localesPaths": ["src/i18n/locales"],
  "i18n-ally.dirStructure": "auto",
  "i18n-ally.sourceLanguage": "ru",
  "i18n-ally.keepFulfilled": true,
  "i18n-ally.extract.keyMaxLength": 200,
  "i18n-ally.extract.targetPickingStrategy": "global-previous",
  "i18n-ally.fullReloadOnChanged": true,
  "i18n-ally.annotationDelimiter": "",
  "i18n-ally.displayLanguage": "ru",
  "i18n-ally.keystyle": "nested",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.tabSize": 2,
  "diffEditor.ignoreTrimWhitespace": false,
  "prettier.trailingComma": "none",
  "prettier.arrowParens": "avoid",
  "formattingToggle.affects": ["formatOnPaste", "formatOnSave", "formatOnType"],
  "eslint.validate": ["vue", "javascript", "javascriptreact"],
  "vetur.validation.template": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "files.eol": "\n"
}
